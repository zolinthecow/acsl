int **truthTable;

int main() {
	string expression;
	getline(cin, expression);
	int variableAmount=0;
	for(unsigned int i=0; i<expression.length(); i++){
		if(expression.at(i)>=65 && expression.at(i)<=90){
			variableAmount++;
		}
	}

}
